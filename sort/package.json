{
  "name": "sort-step-extension",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kaotoio.github.io/step-extension-repository",
  "port": 3002,
  "devDependencies": {
    "@babel/core": "7.22.19",
    "@babel/preset-react": "7.22.15",
    "@babel/preset-typescript": "7.22.15",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "babel-loader": "9.1.3",
    "css-loader": "^6.2.0",
    "html-webpack-plugin": "5.5.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "mini-css-extract-plugin": "^2.7.5",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "null-loader": "^4.0.1",
    "serve": "14.2.1",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.1.1",
    "typescript": "5.2.2",
    "util": "^0.12.5",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@patternfly/patternfly": "4.224.5",
    "@patternfly/react-core": "4.276.12",
    "@patternfly/react-styles": "4.92.8",
    "buffer": "^6.0.3",
    "common": "0.1.0",
    "openapi-types": "^12.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-http": "^3.2.0",
    "web-vitals": "^3.3.1"
  },
  "scripts": {
    "start:dev": "webpack-cli serve --https --hot --color --progress --config webpack.dev.js",
    "start": "webpack-cli serve --hot --color --progress --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.common.js",
    "serve": "serve dist -p 3002",
    "clean": "rm -rf dist",
    "test": "jest --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@patternfly/react-styles)/)"
    ]
  }
}
