    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "5.1.3",
    "util": "^0.12.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@patternfly/patternfly": "4.224.2",
    "@patternfly/react-core": "4.276.8",
    "@patternfly/react-styles": "4.92.6",
    "buffer": "^6.0.3",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "openapi-types": "^12.0.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.14.0",
    "react-scripts": "^5.0.1",
    "stream-http": "^3.2.0",
    "web-vitals": "^3.3.1",
    "yaml": "^2.2.2"
  },
  "scripts": {
    "build": "webpack --mode production --config webpack.common.js",
    "start": "webpack-cli serve --hot --color --progress --config webpack.dev.js",
    "clean": "rm -rf dist",
    "format": "prettier --check 'src'",
    "format:fix": "prettier --write --ignore-unknown 'src'",
    "lint": "eslint --ext .ts,.tsx,.js ./src",
    "lint:fix": "yarn lint --fix",
    "lint-staged": "yarn lint:fix && yarn format:fix",
    "test": "jest --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@patternfly/react-styles|yaml)/)"
    ]
  }
}
